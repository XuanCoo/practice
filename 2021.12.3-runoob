#include<stdio.h>


int main()
{
    // 输出1-100的质数
    int flag;
    for (int i = 2; i<=100; i++)
    {
        flag = 1;
        for (int p = 2; p < i/2; p++)
        {
            if (i!=p){
                if (i%p ==0){
                    flag = 0;
                    break;
                    
                }
            }
        }
                    if (flag == 1) printf("%d\n", i);

    }
}

