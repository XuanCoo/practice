#include<stdio.h>

void reverse(char* s)
{
    // get a printor of char
    int len = 0;
    char* p = s; //give p the location
    while (*p != 0)
    {
        len++;
        p++;
    }
    
    int i = 0;
    char c;//中间体
    while (i <= len / 2 - 1) // -1 > the index = len - 1
    {
        c = *(s+i);
        *(s+i) = *(s+len-1-i);
        *(s+len-1-i) = c;
        i++;
    }
}

int main()
{
    char s[] = "abcd";
    reverse(s);
    
    printf("%s",s);
}










